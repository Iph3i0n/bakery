<meta name="l-row">
  A row to contain `l-col` tags.
  <attr name="cols" type="number" default="12">
    The number of columns to have per row.
  </attr>

  <aria role="grid" />
</meta>

<style>
  :host {
    display: grid;
    grid-template-columns: ":`repeat(${self.props.cols ?? '12'}, minmax(0, 1fr))`";
  }
</style>

<slot></slot>
