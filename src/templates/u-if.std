<meta name="u-if">
  Will only render its children if data retrieved from the context is truthy.
  <attr name="check">
    The JavaScript snippet used to get the data from the context.
  </attr>
  <prop name="data">Data to use instead of items from the use attribute.</prop>
  <base use="Receiver" from="../data.ts" />
</meta>

<script>
  self.accessor = "check";
</script>
<s:if check=":self.data">
  <slot></slot>
</s:if>
