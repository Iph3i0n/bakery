<script>
  import { RenderEvent } from "../deps.ts";
  import { Provider, Receiver } from "../data.ts";
  export const name = "u-relay";
  export const props = ["use", "key"];

  const receiver = new Receiver(self, self.props.use);
  const provider = new Provider(self, self.props.key);

  self.addEventListener(RenderEvent.Key, () => {
    provider.data = receiver.data;
  })
</script>

<slot></slot>
