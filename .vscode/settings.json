{
  "deno.enable": true,
  "deno.unstable": true,
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "files.associations": {
    "*.std": "html",
    "*.pss": "css"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".github": true,
    ".vscode": true,
    "dist": true,
    "deno.lock": true
  }
}
